{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultGradients": false,
      "defaultPalette": false,
      "defaultDuotone": false,
      "duotone": [
        {
          "colors": ["#000000", "#ffffff"],
          "slug": "dark",
          "name": "Dark"
        },
        {
          "colors": ["#ffffff", "#000000"],
          "slug": "light",
          "name": "Light"
        }
      ],
      "gradients": [
        {
          "slug": "hard-vertical-background-1-to-foreground-1",
          "gradient": "linear-gradient(to bottom,var:preset|color|background-1 50%,var:preset|color|foreground-1 50%)",
          "name": "Hard Vertical: background-1 to foreground-1"
        },
        {
          "slug": "hard-vertical-foreground-1-to-background-1",
          "gradient": "linear-gradient(to bottom,var:preset|color|foreground-1 50%,var:preset|color|background-1 50%)",
          "name": "Hard Vertical: foreground-1 to background-1"
        },
        {
          "slug": "hard-horizontal-background-1-to-foreground-1",
          "gradient": "linear-gradient(to right,var:preset|color|background-1 50%,var:preset|color|foreground-1 50%)",
          "name": "Hard Horizontal: background-1 to foreground-1"
        },
        {
          "slug": "hard-horizontal-foreground-1-to-background-1",
          "gradient": "linear-gradient(to right,var:preset|color|foreground-1 50%,var:preset|color|background-1 50%)",
          "name": "Hard Horizontal: foreground-1 to background-1"
        },
        {
          "slug": "hard-vertical-background-1-to-background-2",
          "gradient": "linear-gradient(to bottom,var:preset|color|background-1 50%,var:preset|color|background-2 50%)",
          "name": "Hard Vertical: background-1 to background-2"
        },
        {
          "slug": "hard-vertical-background-2-to-background-1",
          "gradient": "linear-gradient(to bottom,var:preset|color|background-2 50%,var:preset|color|background-1 50%)",
          "name": "Hard Vertical: background-2 to background-1"
        }
      ],
      "palette": [
        {
          "slug": "primary",
          "color": "oklch(52.7% 0.154 150.069)",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "oklch(50% 0.134 242.749)",
          "name": "Secondary"
        },
        {
          "slug": "background-1",
          "color": "oklch(98.4% 0.003 247.858)",
          "name": "Background 1"
        },
        {
          "slug": "background-2",
          "color": "oklch(96.8% 0.007 247.896)",
          "name": "Background 2"
        },
        {
          "slug": "background-3",
          "color": "oklch(86.9% 0.022 252.894)",
          "name": "Background 3"
        },
        {
          "slug": "background-4",
          "color": "oklch(70.4% 0.04 256.788)",
          "name": "Background 4"
        },
        {
          "slug": "foreground-1",
          "color": "oklch(12.9% 0.042 264.695)",
          "name": "Foreground 1"
        },
        {
          "slug": "foreground-2",
          "color": "oklch(20.8% 0.042 265.755)",
          "name": "Foreground 2"
        },
        {
          "slug": "foreground-3",
          "color": "oklch(37.2% 0.044 257.287)",
          "name": "Foreground 3"
        },
        {
          "slug": "foreground-4",
          "color": "oklch(44.6% 0.043 257.281)",
          "name": "Foreground 4"
        },
        {
          "slug": "tertiary",
          "color": "oklch(58.6% 0.253 17.585)",
          "name": "Tertiary"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "xSmall",
          "size": "clamp(0.5rem, 1.5vw, 1rem)",
          "slug": "20"
        },
        {
          "name": "Small",
          "size": "clamp(0.75rem, 2vw, 1.5rem)",
          "slug": "30"
        },
        {
          "name": "Medium",
          "size": "clamp(1rem, 2.5vw, 2rem)",
          "slug": "40"
        },
        {
          "name": "Large",
          "size": "clamp(1.25rem, 4vw, 3rem)",
          "slug": "50"
        },
        {
          "name": "xLarge",
          "size": "clamp(1.5rem, 6.5vw, 4rem)",
          "slug": "60"
        },
        {
          "name": "xxLarge",
          "size": "clamp(2rem, 7.5vw, 6rem)",
          "slug": "80"
        },
        {
          "name": "Giant",
          "size": "clamp(2rem, 8vw, 7.5rem)",
          "slug": "100"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"],
      "blockGap": true,
      "padding": true,
      "margin": true
    },
    "typography": {
      "defaultFontSizes": false,
      "dropCap": false,
      "fluid": true,
      "lineHeight": true,
      "fontSizes": [
        {
          "name": "Small",
          "size": "1.125rem",
          "slug": "small",
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          }
        },
        {
          "name": "Medium",
          "size": "1.5rem",
          "slug": "medium",
          "fluid": {
            "min": "1.125rem",
            "max": "1.5rem"
          }
        },
        {
          "name": "Large",
          "size": "2.25rem",
          "slug": "large",
          "fluid": {
            "min": "1.25rem",
            "max": "2.25rem"
          }
        },
        {
          "name": "xLarge",
          "size": "2.625rem",
          "slug": "x-large",
          "fluid": {
            "min": "1.5rem",
            "max": "3rem"
          }
        },
        {
          "name": "xxLarge",
          "size": "4rem",
          "slug": "xx-large",
          "fluid": {
            "min": "2rem",
            "max": "4rem"
          }
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "base",
          "fontFace": [
            {
              "fontDisplay": "block",
              "fontFamily": "Inter",
              "fontWeight": "100 200 300 400 500 600 700 800 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": ["file:./assets/fonts/inter/Inter.ttf"]
            }
          ]
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var:preset|color|background-1",
      "text": "var:preset|color|foreground-1"
    },
    "spacing": {
      "blockGap": "var:preset|spacing|30",
      "padding": {
        "top": "0px",
        "right": "var:preset|spacing|40",
        "bottom": "0px",
        "left": "var:preset|spacing|40"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|base",
      "lineHeight": "1.6",
      "fontSize": "var:preset|font-size|small",
      "fontWeight": "400"
    },
    "blocks": {
      "core/button": {
        "variations": {
          "outline": {
            "border": {
              "color": "currentColor",
              "width": "2px"
            },
            "css": ":hover{color:var(--wp--preset--color--background-1);background:var(--wp--preset--color--foreground-1);border-color:var(--wp--preset--color--foreground-1);};border-color:var(--wp--preset--color--foreground-1);}",
            "spacing": {
              "padding": {
                "top": "calc(0.75rem - 2px)",
                "right": "calc(var:preset|spacing|40 - 2px)",
                "bottom": "calc(0.625rem - 2px)",
                "left": "calc(var:preset|spacing|40 - 2px)"
              }
            }
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "var:preset|spacing|30"
        }
      },
      "core/comment-author-name": {
        "typography": {
          "fontWeight": "500"
        },
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none",
              "fontSize": "var:preset|font-size|small"
            }
          }
        }
      },
      "core/comment-content": {
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|20"
          }
        }
      },
      "core/comment-date": {
        "typography": {
          "fontSize": "var:preset|font-size|x-small"
        },
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none",
              "fontSize": "var:preset|font-size|x-small"
            },
            "color": {
              "text": "var:preset|color|foreground-3"
            }
          }
        }
      },
      "core/comment-edit-link": {
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|foreground-3"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|primary"
              }
            }
          }
        },
        "typography": {
          "fontSize": "var:preset|font-size|x-small"
        }
      },
      "core/comment-reply-link": {
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|primary"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|foreground-1"
              }
            },
            "typography": {
              "textDecoration": "none",
              "fontWeight": "500"
            }
          }
        },
        "typography": {
          "fontSize": "var:preset|font-size|small"
        }
      },
      "core/comments-title": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        },
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var:preset|spacing|50"
          }
        }
      },
      "core/details": {
        "border": {
          "color": "currentColor",
          "radius": "8px",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      },
      "core/image": {
        "border": {
          "radius": "8px"
        },
        "elements": {
          "caption": {
            "spacing": {
              "margin": {
                "top": "var:preset|spacing|20"
              }
            },
            "typography": {
              "fontSize": "var:preset|font-size|x-small",
              "lineHeight": "1.25"
            },
            "color": {
              "text": "var:preset|color|foreground-3"
            }
          }
        }
      },
      "core/navigation": {
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        },
        "typography": {
          "fontSize": "1rem",
          "textDecoration": "none",
          "fontWeight": "500"
        },
        "spacing": {
          "blockGap": "var:preset|spacing|30"
        }
      },
      "core/post-comments-form": {
        "elements": {
          "heading": {
            "spacing": {
              "margin": {
                "top": "var:preset|spacing|50",
                "bottom": "var:preset|spacing|30"
              }
            },
            "typography": {
              "fontSize": "var:preset|font-size|large"
            }
          }
        }
      },
      "core/post-template": {
        "elements": {
          "h2": {
            "typography": {
              "fontSize": "var:preset|font-size|large",
              "lineHeight": "1.1"
            }
          }
        }
      },
      "core/post-title": {
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|foreground-1"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|primary"
              },
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontWeight": "500",
          "lineHeight": "1.1"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "1px 0"
        }
      },
      "core/query-pagination": {
        "elements": {
          "link": {
            "border": {
              "radius": "8px",
              "width": "0"
            },
            "color": {
              "background": "var:preset|color|tertiary",
              "text": "var:preset|color|foreground-1"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "right": "1.15rem",
                "bottom": "0.5rem",
                "left": "1.15rem"
              }
            },
            "typography": {
              "fontSize": "var:preset|font-size|small",
              "textDecoration": "none"
            },
            ":hover": {
              "color": {
                "background": "var:preset|color|foreground-2",
                "text": "var:preset|color|background-1"
              }
            }
          }
        }
      },
      "core/query-title": {
        "typography": {
          "fontWeight": "500",
          "lineHeight": "1.25",
          "fontSize": "var:preset|font-size|large"
        }
      },
      "core/quote": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "fontStyle": "normal"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|50",
            "right": "0",
            "bottom": "var:preset|spacing|50",
            "left": "0"
          }
        },
        "elements": {
          "cite": {
            "typography": {
              "fontStyle": "normal",
              "fontWeight": "500"
            }
          }
        }
      },
      "core/separator": {
        "border": {
          "color": "currentColor",
          "radius": "0",
          "style": "solid",
          "width": "1px 0 0"
        }
      },
      "core/site-title": {
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|foreground-1"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|primary"
              }
            },
            "typography": {
              "textDecoration": "none",
              "letterSpacing": "0"
            }
          }
        },
        "typography": {
          "letterSpacing": "-0.03em",
          "lineHeight": "1.6",
          "fontSize": "22px",
          "fontWeight": "500"
        }
      },
      "core/spacer": {
        "spacing": {
          "margin": {
            "top": "0"
          }
        }
      }
    },
    "elements": {
      "button": {
        "border": {
          "radius": "8px"
        },
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|background-1"
        },
        ":focus": {
          "outline": {
            "color": "var:preset|color|primary",
            "offset": "2px"
          }
        },
        ":hover": {
          "color": {
            "background": "var:preset|color|foreground-1",
            "text": "var:preset|color|background-1"
          },
          "border": {
            "color": "transparent"
          }
        },
        "spacing": {
          "padding": {
            "top": "0.75rem",
            "right": "var:preset|spacing|40",
            "bottom": "0.625rem",
            "left": "var:preset|spacing|40"
          }
        },
        "typography": {
          "fontWeight": "500"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|base",
          "lineHeight": "1.25",
          "letterSpacing": "-0.03em",
          "fontWeight": "500"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|40"
          }
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|60"
          }
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|50"
          }
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|small"
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|primary"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "color": {
            "text": "var:preset|color|foreground-1"
          },
          "typography": {
            "textDecoration": "none"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "sidebar",
      "title": "Sidebar",
      "area": "sidebar"
    }
  ],
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank",
      "postTypes": ["page", "post"]
    },
    {
      "name": "page-no-title",
      "title": "No Title",
      "postTypes": ["page"]
    }
  ]
}
