{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"custom": {
			"lineHeight": {
				"one": 1,
				"tiny": 1.1,
				"small": 1.25,
				"medium": 1.4,
				"normal": 1.6,
				"large": 2
			},
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"borderRadius": {
				"tiny": "3px",
				"small": "8px",
				"medium": "24px",
				"large": "clamp(1.25rem, 2.5vw, 2rem)",
				"round": "9999px",
				"circle": "50%"
			}
		},
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"defaultDuotone": false,
			"duotone": [
				{
					"colors": [
						"#000000",
						"#ffffff"
					],
					"slug": "dark",
					"name": "Dark"
				},
				{
					"colors": [
						"#ffffff",
						"#000000"
					],
					"slug": "light",
					"name": "Light"
				}
			],
			"gradients": [
				{
					"slug": "hard-vertical-background-1-to-foreground-1",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--background-1) 50%,var(--wp--preset--color--foreground-1) 50%)",
					"name": "Hard Vertical: background-1 to foreground-1"
				},
				{
					"slug": "hard-vertical-foreground-1-to-background-1",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--foreground-1) 50%,var(--wp--preset--color--background-1) 50%)",
					"name": "Hard Vertical: foreground-1 to background-1"
				},
				{
					"slug": "hard-horizontal-background-1-to-foreground-1",
					"gradient": "linear-gradient(to right,var(--wp--preset--color--background-1) 50%,var(--wp--preset--color--foreground-1) 50%)",
					"name": "Hard Horizontal: background-1 to foreground-1"
				},
				{
					"slug": "hard-horizontal-foreground-1-to-background-1",
					"gradient": "linear-gradient(to right,var(--wp--preset--color--foreground-1) 50%,var(--wp--preset--color--background-1) 50%)",
					"name": "Hard Horizontal: foreground-1 to background-1"
				},
				{
					"slug": "hard-vertical-background-1-to-background-2",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--background-1) 50%,var(--wp--preset--color--background-2) 50%)",
					"name": "Hard Vertical: background-1 to background-2"
				},
				{
					"slug": "hard-vertical-background-2-to-background-1",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--background-2) 50%,var(--wp--preset--color--background-1) 50%)",
					"name": "Hard Vertical: background-2 to background-1"
				}
			],
			"palette": [
				{
					"slug": "primary",
					"color": "#111111",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#EFEFEF",
					"name": "Secondary"
				},
				{
					"slug": "background-1",
					"color": "#FFFFFF",
					"name": "Background 1"
				},
				{
					"slug": "background-2",
					"color": "#FBFBFB",
					"name": "Background 2"
				},
				{
					"slug": "background-3",
					"color": "#F5F5F5",
					"name": "Background 3"
				},
				{
					"slug": "background-4",
					"color": "#EDEDED",
					"name": "Background 4"
				},
				{
					"slug": "foreground-1",
					"color": "#111111",
					"name": "Foreground 1"
				},
				{
					"slug": "foreground-2",
					"color": "#333333",
					"name": "Foreground 2"
				},
				{
					"slug": "foreground-3",
					"color": "#555555",
					"name": "Foreground 3"
				},
				{
					"slug": "foreground-4",
					"color": "#777777",
					"name": "Foreground 4"
				},
				{
					"slug": "tertiary",
					"color": "#EFEFEF",
					"name": "Tertiary"
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "xSmall",
					"size": "clamp(0.5rem, 1.5vw, 1rem)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "clamp(0.75rem, 2vw, 1.5rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1rem, 2.5vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, 4vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "xLarge",
					"size": "clamp(1.5rem, 6.5vw, 4rem)",
					"slug": "x-large"
				},
				{
					"name": "xxLarge",
					"size": "clamp(2rem, 7.5vw, 6rem)",
					"slug": "xx-large"
				},
				{
					"name": "Giant",
					"size": "clamp(2rem, 8vw, 7.5rem)",
					"slug": "giant"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"blockGap": true,
			"padding": true,
			"margin": true
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "base",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontWeight": "100 200 300 400 500 600 700 800 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/inter/Inter.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1.125rem",
					"slug": "small",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium",
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem"
					}
				},
				{
					"name": "Large",
					"size": "2.25rem",
					"slug": "large",
					"fluid": {
						"min": "1.25rem",
						"max": "2.25rem"
					}
				},
				{
					"name": "xLarge",
					"size": "2.625rem",
					"slug": "x-large",
					"fluid": {
						"min": "1.5rem",
						"max": "3rem"
					}
				},
				{
					"name": "xxLarge",
					"size": "4rem",
					"slug": "xx-large",
					"fluid": {
						"min": "2rem",
						"max": "4rem"
					}
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		}
	},
	"styles": {
		"blocks": {
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground-1)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"border": {
							"radius": "var(--wp--custom--border-radius--small)",
							"width": "0"
						},
						"color": {
							"background": "var(--wp--preset--color--tertiary)",
							"text": "var(--wp--preset--color--foreground-1)"
						},
						"spacing": {
							"padding": {
								"top": "0.5rem",
								"right": "1.15rem",
								"bottom": "0.5rem",
								"left": "1.15rem"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--foreground-2)",
								"text": "var(--wp--preset--color--background-1)"
							}
						}
					}
				}
			},
			"core/post-template": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)",
							"lineHeight": "var(--wp--custom--line-height--tiny)"
						}
					}
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none",
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/comment-content": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-small)"
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none",
							"fontSize": "var(--wp--preset--font-size--x-small)"
						},
						"color": {
							"text": "var(--wp--preset--color--foreground-3)"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground-3)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"core/comment-reply-link": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--foreground-1)"
							}
						},
						"typography": {
							"textDecoration": "none",
							"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/post-comments-form": {
				"elements": {
					"heading": {
						"spacing": {
							"margin": {
								"top": "var(--wp--preset--spacing--large)",
								"bottom": "var(--wp--preset--spacing--small)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"elements": {
					"caption": {
						"spacing": {
							"margin": {
								"top": "var(--wp--preset--spacing--x-small)"
							}
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-small)",
							"lineHeight": "var(--wp--custom--line-height--small)"
						},
						"color": {
							"text": "var(--wp--preset--color--foreground-3)"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--large)",
						"right": "0",
						"bottom": "var(--wp--preset--spacing--large)",
						"left": "0"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontStyle": "normal",
							"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground-1)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"typography": {
							"textDecoration": "none",
							"letterSpacing": "0"
						}
					}
				},
				"typography": {
					"letterSpacing": "-0.03em",
					"lineHeight": "var(--wp--custom--line-height--normal)",
					"fontSize": "22px",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				},
				"typography": {
					"fontSize": "1rem",
					"textDecoration": "none",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			}
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"spacing": {
					"padding": {
						"top": "0.7rem",
						"right": "clamp(1.5rem, 1.75vw, 3.5rem)",
						"bottom": "0.6rem",
						"left": "clamp(1.5rem, 1.75vw, 3.5rem)"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--medium)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background-1)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--foreground-1)",
						"text": "var(--wp--preset--color--background-1)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--foreground-1)",
						"text": "var(--wp--preset--color--background-1)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--foreground-1)",
						"text": "var(--wp--preset--color--background-1)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--base)",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"letterSpacing": "-0.03em",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--foreground-1)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background-1)",
			"text": "var(--wp--preset--color--foreground-1)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--small)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--preset--spacing--large)",
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--large)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--base)",
			"lineHeight": "var(--wp--custom--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--regular)"
		}
		
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "sidebar"
		}
	],
	"customTemplates": [
		{
			"name": "rckbs-child-custom-template",
			"title": "Custom Template",
			"postTypes": [
				"page",
				"post"
			]
		}
	]
}